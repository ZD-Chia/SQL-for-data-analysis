##Quiz write your first subquery
1. SELECT channel,
	   DATE_TRUNC('day',occurred_at),
       COUNT(*)
    FROM web_events
    GROUP BY 1,2
    ORDER BY 1
    
    --SELECT DATE_TRUNC('day',occurred_at) AS day,
       channel, COUNT(*) as events
    FROM web_events
    GROUP BY 1,2
    ORDER BY 3 DESC;--
    
    
    
2. SELECT *
    FROM
      (SELECT channel,
	     DATE_TRUNC('day',occurred_at),
       COUNT(*) AS events_occur
        FROM web_events
        GROUP BY 1,2
        ORDER BY 3 DESC) sub
        
        --SELECT *
          FROM (SELECT DATE_TRUNC('day',occurred_at) AS day,
                channel, COUNT(*) as events
          FROM web_events 
          GROUP BY 1,2
          ORDER BY 3 DESC) sub;--
        
        
        
3. SELECT  channel, avg(events_occur)
    FROM
      (SELECT channel,
	    DATE_TRUNC('day',occurred_at),
      COUNT(*) AS events_occur
      FROM web_events
      GROUP BY 1,2
      ORDER BY 3 DESC) sub
    GROUP BY 1
    ORDER BY 1
    
    --SELECT channel, AVG(events) AS average_events
      FROM (SELECT DATE_TRUNC('day',occurred_at) AS day,
                channel, COUNT(*) as events
         FROM web_events 
         GROUP BY 1,2) sub
      GROUP BY channel
      ORDER BY 2 DESC;--
      
      
      
